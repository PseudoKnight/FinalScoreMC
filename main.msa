*:/stack = >>>
	if(!has_permission('group.moderators'), die('You do not have permission.'))
	@inv = pinv()
	for(@i = 0, @i < 36, @i++,
		if(@inv[@i] == null || @inv[@i]['qty'] >= 64, continue())
		for(@y = @i + 1, @y < 36, @y++,
			if(@inv[@y] == null, continue())
			if(@inv[@i]['type'] == @inv[@y]['type']
			&& @inv[@i]['data'] == @inv[@y]['data']
			&& @inv[@i]['enchants'] == @inv[@y]['enchants']
			&& @inv[@i]['meta'] == @inv[@y]['meta']) {
				@extra = @inv[@i]['qty'] + @inv[@y]['qty'] - 64
				if(@extra < 1) {
					@inv[@i]['qty'] = @inv[@i]['qty'] + @inv[@y]['qty']
					@inv[@y] = null
				} else {
					@inv[@i]['qty'] = 64
					@inv[@y]['qty'] = @extra
				}
			}
		)
	)
	set_pinv(@inv)
	msg('Stacked!')
<<<

