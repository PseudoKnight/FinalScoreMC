			if(_acc_subtract($player, $amount)) {
				msg('Subtracted '.$amount.' coins from '.$player)
			} else {
				msg('Can\'t find player.')
			}
		, 'info',
			if(@coins = _acc_balance($player)) {
				msg(color('gold').round(_acc_balance($player)).' coins')
			} else {
				msg('Can\'t find player.')
			}
		)
	}
